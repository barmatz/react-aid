{
  "extends": "airbnb-base",
  "env": {
    "node": true
  },
  "rules": {
    "no-console": "off",
    "no-trailing-spaces": "off",
    "no-shadow": "off",
    "one-var": "off",
    "indent": [ "error", 2, {
      "VariableDeclarator": {
        "var": 2,
        "let": 2,
        "const": 2
      }
    }],
    "comma-style": [ "error", "first", {
      "exceptions": {
        "ArrayExpression": true,
        "ObjectExpression": true
      }
    }]
  }
}